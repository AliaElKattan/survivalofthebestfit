{
  "name": "survivalofthebestfit",
  "version": "1.0.0",
  "description": "Simulation teaching about problems in AI",
  "main": "app.js",
  "scripts": {
    "test": "test scrip include from karma website",
    "start": "parcel static-website-content/index.html",
    "dev": "nodemon app.js",
    "dev-all": "npm-run-all --parallel watch:* dev",
    "build:css": "node-sass --include-path scss public/scss/main.scss public/css/main.css",
    "watch:css": "nodemon -e scss -x \"npm run build:css\"",
    "watch:main-js": "watchify public/js/main.js -t babelify --outfile public/dist/main-bundle.js -v --debug",
    "watch:transition-js": "watchify public/js/transition-page/index.js -t babelify --outfile public/dist/transition-bundle.js -v"
  },
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "component-loader-js": "^4.0.0",
    "debounce": "^1.2.0",
    "express": "^4.16.4",
    "jquery": "^3.3.1",
    "morgan": "^1.9.1",
    "mustache-express": "^1.2.8",
    "nodemon": "^1.18.7",
    "pixi-tween": "^0.2.0",
    "pixi.js": "^4.8.5",
    "pug": "^2.0.3",
    "watchify": "^3.11.0"
  },
  "devDependencies": {
    "jasmine-core": "^3.3.0",
    "karma": "^3.1.3",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^2.0.1",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.11.0"
  },
  "author": "",
  "license": "MIT"
}
