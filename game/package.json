{
  "name": "survivalofthebestfit",
  "version": "1.0.0",
  "description": "Simulation teaching about problems in AI",
  "main": "app.js",
  "config": {},
  "nodemonConfig": {
    "ignore": ["**/fontawesome-free/*", "node_modules/*"]
  },
  "scripts": {
    "test": "test scrip include from karma website",
    "dev": "nodemon app.js",
    "dev-all": "npm-run-all --parallel clean-dist watch:* dev ",
    "clean-dist": "mkdir public/dist ; del-cli public/dist/*",
    "build:css": "node-sass --include-path node_modules/sass-mq public/scss/main.scss public/css/main.css",
    "watch:css": "nodemon -e scss -x \"npm run build:css\"",
    "watch:main-js": "watchify public/controllers/game/gameEntry.js -t babelify --outfile public/dist/main-bundle.js -v --debug"
  },
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "component-loader-js": "^4.0.0",
    "debounce": "^1.2.0",
    "express": "^4.16.4",
    "gsap": "^2.1.2",
    "jquery": "^3.3.1",
    "libsvm-js": "^0.2.1",
    "lodash": "^4.17.11",
    "machina": "^4.0.2",
    "morgan": "^1.9.1",
    "mustache-express": "^1.2.8",
    "nodemon": "^1.18.7",
    "pixi-timer": "^1.0.1",
    "pixi-tween": "^0.2.0",
    "pixi.js": "^4.8.5",
    "pug": "^2.0.3",
    "sass-mq": "^5.0.0",
    "watchify": "^3.11.1"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "del-cli": "^1.1.0",
    "eslint": "^5.14.0",
    "eslint-config-google": "^0.12.0",
    "jasmine-core": "^3.3.0",
    "karma": "^3.1.3",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^2.0.1",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5"
  },
  "author": "",
  "license": "MIT"
}
